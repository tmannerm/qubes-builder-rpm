[qubes-builder-dom0-current]
name = Qubes Dom0 Repository (updates)
baseurl = https://yum.qubes-os.org/r%QUBESVER%/current/dom0/%DIST%
enabled = 1
metadata_expire = 7d
gpgcheck = 1
repo_gpgcheck = 1
gpgkey = file://$sysroot/etc/pki/rpm-gpg/RPM-GPG-KEY-qubes-%QUBESVER%-primary
exclude = qubes-release

